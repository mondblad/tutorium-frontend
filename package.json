{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:docker": "tsc -b && vite build --mode docker",
    "build:production": "tsc -b && vite build --mode production",
    "lint": "eslint .",
    "preview": "vite preview",
    "apigen:auth": "openapi --input ./src/api/openapi/auth-service.json --output ./src/api/generate/auth-service --client axios && barrelsby --directory src/api/generate/auth-service/services --delete",
    "apigen:user": "openapi --input ./src/api/openapi/user-service.json --output ./src/api/generate/user-service --client axios && barrelsby --directory src/api/generate/user-service/services --delete",
    "apigen": "npm-run-all apigen:*"
  },
  "dependencies": {
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "form-data": "^4.0.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "barrelsby": "^2.8.1",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript-codegen": "^0.29.0",
    "swagger-typescript-api": "^13.2.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.6"
  }
}
